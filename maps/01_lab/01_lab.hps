	}
	
	void shakeMapPer()
	{
		Map_AddTimer("timer_StartPerMapShake",cMath_RandRectf(45.0f,360.0f),"timer_StartPerMapShake");
		if(mbInTunnel_climb || mbInTunnel_shortcut)
		{
			cLux_AddDebugMessage("debug: periodic shake in tunnel");
			Map_AddTimer("timer_shake_sfx_hatch",0.1f,"timer_shake_sfx_hatch");
		}
		else if(bIsPlayerInCave)
		{
			cLux_AddDebugMessage("debug: periodic shake in cave");
			Map_AddTimer("timer_shake_sfx_cave",0.1f,"timer_shake_sfx_cave");
		}
		else
		{
			cLux_AddDebugMessage("debug: periodic shake");
			Map_AddTimer("timer_shake_sfx",0.1f,"timer_shake_sfx");
		}
		
		Effect_Shake_Start(0.05, 0.05, 0.4, 2.5f);
	}
	
