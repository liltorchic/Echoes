	void computer_panel_door_control_OnGui(const tString&in asEntityName, float afTimeStep)
	{
		if(t_marked)
		{
			loadingTime = 0.85f;
		}

			ImGui_SetTransCategory("Security_Computer");
	
			StationGuiBG_Scanlines();
			StationGuiBG_Backdrop();
			
			StationGuiBG_Taskbar("theta");
			
			if (StationGui_DrawAndCheckErrors()) return;
			
			if(!omni_inserted)
			{
				StationGui_SetActiveApp(eCTA_Splash);
				StationGui_BigStatusMessage_Old("TechTermLocked", gStationGuiColor_Warning, 0.875);
			}
			else
			{
				int lActiveApp = StationGui_GetActiveApp(eCTA_Splash);
				bool bBackButton = true;
				switch(lActiveApp)
				{
					
					//initial unlocked screen
					case eCTA_Splash:
						if(StationGui_LoadingWindow("loading_terminal", "ComputerApp_Door_Reading_Caption", "", loadingTime, true,cVector2f(0.6,0.2),1.5f))
						{
							Sound_CreateAtEntity("sfx_terminal_welcome", "Entities_Station/tech/terminal_beeps/pathOS_comp/pathOS_comp_ granted", asEntityName, 0.0f, false, 1.0f);
							StationGui_SetActiveApp(eCTA_MainMenu);
						}

						bBackButton = false;
					break;
					
					//main menu
					case eCTA_MainMenu:
						StationGui_AddApp("ComputerApp_Door_Control_Title",omni_inserted,eCTA_Loading);
						StationGui_AddApp("ComputerApp_Door_Control_ADMIN",false,eCTA_Loading);
						StationGui_AddApp("ComputerApp_Door_Control_COMS",false,eCTA_Loading);
						StationGui_MainMenu();
						bBackButton = false;
					break;
			
					//loading into door control
					case eCTA_Loading:
                    
						if(StationGui_LoadingWindow("loading_terminal", "ComputerApp_Door_Loading_Caption", "ComputerApp_Door_Loading_Text", loadingTime, true,cVector2f(0.6,0.2),1.5f))
						{
							t_marked = true;
							Sound_CreateAtEntity("sfx_terminal_welcome", "Entities_Station/tech/terminal_beeps/omicron_lockdown_puzzle_button", asEntityName, 0.0f, false, 1.0f);
							StationGui_SetActiveApp(eCTA_Control);
						}
						bBackButton = false;
					break;
				
				
					//door control
					case eCTA_Control:
						DoDoorControl(asEntityName);
						bBackButton = true;
					break;
					


				}
		
				StationGui_BackButton(bBackButton);
			}
			
			


	}
	
