	// To get when player makes input (mostly used for debug)
	void OnAction(int alAction, bool abPressed) 
	{
		if(alAction == eAction_Test1 && abPressed)
		{
			restorePower();
		}
		if(alAction == eAction_Test2 && abPressed)
		{
			PreloadMap();
		}
		
		if(alAction == eAction_Flashlight && bPickedUpIPAD)
		{
			bPressedFlashlight = true;
			
			if(!bInIPAD)
			{
				Entity_CallEntityInteract("ipad","",cVector3f_Zero,"");//brings up ipad if we have it
			}
		}
		
		if(abPressed==false)
		{
			bPressedFlashlight = false;
			return;
		}
	}
	
