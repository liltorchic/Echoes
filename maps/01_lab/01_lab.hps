	bool dw_hatch_shortcut(const tString &in asParent, const tString &in asChild, int alState)
	{
		if(alState == 1)
		{
			mbInTunnel_shortcut = true;
			cLux_AddDebugMessage("Entered tunnels");
			
			if(bExitedNearHatch)
			{
				iPlayerLocPrev = iPlayerLoc;
			}
			
			iPlayerLoc = -10;//vent
			
		}
		else if(alState == -1)
		{
			mbInTunnel_shortcut = false;
			cLux_AddDebugMessage("Left tunnels");
			
			if(bExitedNearHatch)
			{
				iPlayerLoc = iPlayerLocPrev;
				
			}else
			{
				iPlayerLoc = 9;
			}
		}
		
		Entity_SetActive("climbArea_hatch_airvent_*", !mbInTunnel_shortcut);
		Player_SetJumpDisabled(mbInTunnel_shortcut);

		return true;
	}
	
	void hatch_room_button_1_OnConnectionStateChange(const tString &in asEntity, int alState)
	{
		if (alState == 1)
		{//open
			if (!mbInTunnel_shortcut)
			{
				Map_AddTimer("climbArea_hatch_shortcut_room", 0.75, "TimerActivateEntity");
				Map_AddTimer("climbArea_hatch_shortcut_room", 15.75, "TimerDectivateEntity");
			}
				
		}
		else
